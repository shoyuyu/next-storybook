{
  "name": "next-ts-storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:ts": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:css": "stylelint '**/*.scss'",
    "format": "prettier --write",
    "storybook": "run-p storybook:dev sass:watch",
    "storybook:dev": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sass": "sass src/scss/:dest/components --no-source-map",
    "sass:watch": "sass src/scss/:dest/components --no-source-map --watch"
  },
  "dependencies": {
    "next": "^14.2.33",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "storybook": "^7.6.17",
    "style-loader": "^3.3.4",
    "stylelint": "^16.1.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.2.1",
    "typescript": "^5"
  },
  "overrides": {
    "tar-fs": "^3.1.1"
  }
}
